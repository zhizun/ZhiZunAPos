<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white" 
    android:orientation="vertical"
    >

    <com.ch.epw.view.TitleBarView
        android:id="@+id/title_bar_activity_zxdt_teacher"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <RelativeLayout
        android:id="@+id/rl_my_notice_teacher_menu"
        android:layout_width="match_parent"
        android:layout_height="50.0dip"
        android:background="@color/simple_eeeeee"
        android:gravity="center_vertical" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/tv_my_attence_teacher_texttime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/title_leveltwo_text_size_18" />
        </LinearLayout>
    </RelativeLayout>

    <include layout="@layout/list_attence_js_edit_item" 
        
        />

</LinearLayout>